{
  "database": {
    "rules": "firebase.rule"
  },
  "projects": {
    "default": "inseller-dcac6"
  },
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
